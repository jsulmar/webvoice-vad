{
    "name": "webvoice-vad",
    "version": "0.0.3",
    "description": "A Voice Activity Detector for Node.js",
    "author": "Joe Sulmar - jsulmar",
    "keywords": [
        "voice-activity-detection"
    ],
    "main": "src/webvoicesdk.js",
    "module": "src/webvoicesdk.js",
    "browser": "dist/webvoice-vad.min.js",
    "scripts": {
        "build": "parcel build src/webvoicesdk.js --log-level 4 --no-cache --no-source-maps --detailed-report --out-file webvoice-vad.min.js",
        "test": "parcel tests/with-bundler/index.html --out-dir dev-build --log-level 4 --no-cache"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jsulmar/webvoice-vad.git"
    },
    "license": "",
    "bugs": {
        "url": "https://github.com/jsulmar/webvoice-vad/issues"
    },
    "homepage": "https://github.com/jsulmar/webvoice-vad#readme",
    "devDependencies": {
        "@tensorflow/tfjs": "^3.21.0",
        "@tensorflow/tfjs-backend-cpu": "^3.21.0",
        "@tensorflow/tfjs-backend-wasm": "^3.21.0",
        "@tensorflow/tfjs-backend-webgl": "^3.21.0",
        "parcel-bundler": "^1.12.5",
        "parcel-plugin-tensorflowjs-blob": "^1.0.0",
        "parcel-plugin-wasm-blob": "^1.0.0",
        "parcel-plugin-worker-blob": "^1.0.0"
    },
    "browserslist": [
        "since 2017-06"
    ]
}
