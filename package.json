{
  "name": "webvoicesdk",
  "version": "1.0.0",
  "description": "Buildings block for voice-enabled applications in the browser",
  "main": "index.js",
  "scripts": {
    "build": "parcel build src/webVoiceSDK.js --log-level 4 --no-cache --no-source-maps --detailed-report --out-file webVoiceSDK-linto.min.js && cp dist/webVoiceSDK-linto.min.js tests/static-html/",
    "test": "parcel tests/with-bundler/index.html --out-dir dev-build --log-level 4 --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linto-ai/WebVoiceSDK.git"
  },
  "author": "Damien Laine - LINAGORA",
  "license": "AGPLV3",
  "bugs": {
    "url": "https://github.com/linto-ai/WebVoiceSDK/issues"
  },
  "homepage": "https://github.com/linto-ai/WebVoiceSDK#readme",
  "devDependencies": {
    "@tensorflow/tfjs": "^2.3.0",
    "@tensorflow/tfjs-backend-cpu": "^2.3.0",
    "@tensorflow/tfjs-backend-wasm": "^2.3.0",
    "@tensorflow/tfjs-backend-webgl": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-tensorflowjs-blob": "^1.0.0",
    "parcel-plugin-wasm-blob": "^1.0.0",
    "parcel-plugin-worker-blob": "^1.0.0",
    "rnnoise-wasm": "github:jitsi/rnnoise-wasm.git"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
